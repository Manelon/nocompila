    "version": "0.2.0",
    "configurations": [

        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "default profile",
            "request": "launch",
            "mainClass": "com.manelon.config.ConfigApplication",
            "projectName": "config"
        },
        {
            "type": "java",
            "name": "dev Profile",
            "request": "launch",
            "mainClass": "com.manelon.config.ConfigApplication",
            "projectName": "config",
            "args": "--spring.profile.active=dev"
        }
    ]
}